<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>jee</display-name>
		
	

	<servlet>
	 	<servlet-name>HomeController</servlet-name>
		<servlet-class>controllers.HomeController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HomeController</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	
	
		<servlet>
	 	<servlet-name>TrajectController</servlet-name>
		<servlet-class>controllers.TrajectController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TrajectController</servlet-name>
		<url-pattern>/traject</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
	 	<servlet-name>LoginController</servlet-name>
		<servlet-class>controllers.LoginController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginController</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	
	
	  
	<servlet-mapping>
		<servlet-name>CityApi</servlet-name>
		<url-pattern>/api/cities</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>CityApi</servlet-name>
		<servlet-class>api.CityApi</servlet-class>
	</servlet>
	
	<servlet-mapping>
	    <servlet-name>default</servlet-name>
	    <url-pattern>/resources/*</url-pattern>
	</servlet-mapping>
		
</web-app>